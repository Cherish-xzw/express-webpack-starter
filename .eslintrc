{
  "parser": "babel-eslint",

  "extends": ["airbnb", "prettier"],

  "plugins": ["prettier"],

  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "jquery": true
  },

  "rules": {
    "import/first":"off",
    "no-underscore-dangle":"off",
    "consistent-return": "off"
  }
}

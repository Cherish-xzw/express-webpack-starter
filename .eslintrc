{
  "parser": "babel-eslint",

  "extends": ["airbnb", "prettier"],

  "plugins": ["prettier"],

  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "jquery": true
  },

  "globals": {
    "__publicPath": true
  },

  "rules": {
    "import/first":"off",
    "no-underscore-dangle":"off",
    "consistent-return": "off",
    "linebreak-style":"off",
    "no-new":"off",
    "react/jsx-filename-extension": "off",
    "react/jsx-tag-spacing": "off",
    "react/prefer-stateless-function": "off"
  }
}

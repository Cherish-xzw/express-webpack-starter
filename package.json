{
  "name": "express-webpack-starter",
  "app": "WebServer",
  "version": "0.0.1",
  "description": "Using webpack in multipage application",
  "license": "MIT",
  "basePath": "/",
  "main": "dist/index.js",
  "engines": {
    "node": ">=6.3",
    "npm": ">=3.10"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9",
    "iOS >= 8",
    "Android >= 4"
  ],
  "author": {
    "name": "travisxu",
    "email": "xuzhengwei.amor@gmail.com"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --no-ignore --fix",
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint --fix",
      "git add --force"
    ]
  },
  "precommit": [
    "lint:staged",
    "lint"
  ],
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "all": false
  },
  "scripts": {
    "dev": "concurrently -k \"npm run dev:client\" \"npm run dev:server\"",
    "start": "npm run build && cross-env NODE_ENV=production node dist/index.js",
    "build": "npm-run-all clean --parallel build:*",
    "release": "npm-run-all clean build release:copy release:deps release:package",
    "test": "mocha --compilers ts:ts-node/register -r should --exit",
    "clean": "rimraf public/assets dist release",
    "lint": "npm-run-all lint:js lint:css",
    "lint:fix": "tslint --fix -c tslint.json -p tsconfig.json && git add .",
    "lint:js": "tslint -c tslint.json -p tsconfig.json",
    "lint:css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint:staged": "lint-staged",
    "test:cover": "nyc npm run test",
    "dev:server": "cross-env DEBUG=app:* nodemon --watch src/ --ignore src/assets --exec ts-node src/index.ts",
    "dev:client": "nodemon --watch 'config/webpack.config.js' --exec 'webpack-dev-server --config config/webpack.config.js'",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "build:server": "tsc && cp -r src/views dist",
    "release:copy": "cross-env-shell \"shx mkdir -p release/$npm_package_app && shx cp -r *.json dist/ public/ release/$npm_package_app\"",
    "release:deps": "cross-env-shell \"cd release/$npm_package_app && npm install --production\"",
    "release:package": "cross-env-shell \"cd release && tar -zcvf $npm_package_app.tgz $npm_package_app\""
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.6.8",
    "@types/supertest": "^2.0.8",
    "@types/webpack-env": "^1.14.0",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.1.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.0.1",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.5",
    "glob": "^7.1.3",
    "html-loader": "^0.5.1",
    "http-proxy-middleware": "^0.17.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-loader": "^2.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "should": "^13.2.0",
    "shx": "^0.2.2",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^18.0.0",
    "supertest": "^3.0.0",
    "ts-loader": "^6.0.4",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3",
    "url-loader": "^0.6.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.38.0",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-partials": "^0.3.0",
    "express-validator": "^5.3.1",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "source-map-support": "^0.5.0"
  }
}

{
  "name": "react-admin",
  "version": "0.0.1",
  "description": "Using webpack in multipage application",
  "license": "MIT",
  "path": "/",
  "main": "dist/server.js",
  "engines": {
    "node": ">=6.3",
    "npm": ">=3.10"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 9",
    "iOS >= 8",
    "Android >= 4"
  ],
  "author": {
    "name": "travisxu",
    "email": "xuzhengwei.amor@gmail.com"
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --no-ignore --fix",
      "prettier --write",
      "git add --force"
    ],
    "*.{css,less,styl,scss,sass,sss}": [
      "stylelint --fix",
      "git add --force"
    ]
  },
  "precommit": [
    "lint:staged",
    "lint"
  ],
  "scripts": {
    "dev": "concurrently -k \"npm run dev:client\" \"npm run dev:server\"",
    "start": "npm run build && cross-env NODE_ENV=production node dist/server.js",
    "build": "npm-run-all clean --parallel build:*",
    "release": "npm-run-all clean build release:copy release:deps release:package",
    "deploy": "npm-run-all clean build release:docker",
    "test": "mocha -r babel-register -r should --exit",
    "clean": "rimraf public/assets dist release",
    "lint": "npm-run-all lint:js lint:css",
    "lint:js": "eslint src --ignore-pattern src/proxy",
    "lint:css": "stylelint \"src/**/*.{css,less,styl,scss,sass,sss}\"",
    "lint:staged": "lint-staged",
    "test:cover": "nyc npm run test",
    "dev:server": "cross-env DEBUG=app:* nodemon --watch src/ --ignore src/assets --exec babel-node src/server.js",
    "dev:client": "nodemon --watch config/webpack.config.js -- ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config config/webpack.config.js",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.config.js",
    "build:server": "cross-env BABEL_ENV=production babel src -d dist --ignore src/assets --copy-files -s",
    "release:copy": "cross-env-shell \"shx mkdir -p release/$npm_package_name && shx cp -r package.json dist/ public/ release/$npm_package_name\"",
    "release:deps": "cross-env-shell \"cd release/$npm_package_name && npm install --production\"",
    "release:package": "cross-env-shell \"cd release && tar -zcvf $npm_package_name.tar.gz $npm_package_name\"",
    "release:docker": "cross-env-shell \"docker build -t zhengweixu/$npm_package_name . \""
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.6.7",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "compression-webpack-plugin": "^1.0.1",
    "concurrently": "^3.5.0",
    "copy-webpack-plugin": "^4.2.0",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "http-proxy-middleware": "^0.17.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "lint-staged": "^6.0.0",
    "mocha": "^4.1.0",
    "nodemon": "^1.12.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.4.1",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.9.2",
    "rimraf": "^2.6.2",
    "should": "^13.2.0",
    "shx": "^0.2.2",
    "style-loader": "^0.19.0",
    "stylelint": "^8.2.0",
    "stylelint-config-standard": "^18.0.0",
    "supertest": "^3.0.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.5.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.9.4"
  },
  "dependencies": {
    "antd": "^3.4.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "ejs": "^2.5.7",
    "express": "^4.16.2",
    "express-partials": "^0.3.0",
    "history": "^4.7.2",
    "moment": "^2.20.1",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.1",
    "qs": "^6.5.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "source-map-support": "^0.5.0",
    "whatwg-fetch": "^2.0.4"
  }
}
